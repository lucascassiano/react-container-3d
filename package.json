  "jest": {
    "moduleFileExtensions": [
      "jsx",
      "js",
      "json"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(lib|docs|node_modules)/"
    ]
  },
  "dependencies": {
    "react-cubeview": "^2.5.8",
    "react-sizeme": "^2.3.6",
    "three": "^0.88.0",
    "three-react-obj-loader": "^1.0.4"
  }
}
